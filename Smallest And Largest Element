#include<iostream>
using namespace std;

int main() {

    int arr[5] = {50,20,10,3,40};

    int smallest = arr[0];
    int largest = arr[0];
    int n = 5;

    for(int i=0; i<n; i++){

        if(arr[i] < smallest){
            smallest = arr[i];
        }
        else if(arr[i] > largest){
            largest = arr[i];
        }
        
    }

    cout<<"Largest Element: "<<largest<<endl;
    cout<<"Smallest Element: "<<smallest<<endl;

    return 0;
}
