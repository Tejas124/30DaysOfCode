#include<iostream>
#include<stack>
using namespace std;

void reverse(stack<char> &s){

    //base case
    if(s.empty()) return;

    char element = s.top();
    s.pop();
    
    cout<<element;

    reverse(s);

}

int main(){

    string str = "Hello";
    stack<char> s;

    int n = str.length();
    
    for(int i=0; i<n; i++){
        s.push(str[i]);
    }

    reverse(s);


    return 0;
}
